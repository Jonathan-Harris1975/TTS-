# Required for service-account auth (JSON string)
# GOOGLE_CREDENTIALS='{"type":"service_account","project_id":"...","private_key_id":"...","private_key":"-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n","client_email":"...","client_id":"...","token_uri":"https://oauth2.googleapis.com/token"}'

# Or set PASS_THROUGH_AUTH=true and send Authorization: Bearer <token> from upstream
PASS_THROUGH_AUTH=false

# Project and location
GCP_PROJECT_ID=your-gcp-project
GCP_LOCATION=us-central1

# Server
PORT=3000
